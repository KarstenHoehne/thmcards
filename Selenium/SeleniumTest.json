{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "http://khthmcards2.cloudcontrolled.com/login"
    },
    {
      "type": "waitForTextPresent",
      "text": "About THMcards"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Was ist THMcards?"
      }
    },
    {
      "type": "waitForElementPresent",
      "locator": {
        "type": "css selector",
        "value": "img"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "button.close"
      }
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}