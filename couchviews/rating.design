{
   "_id": "_design/rating",
   "language": "javascript",
   "views": {
       "by_id": {
           "map": "function(doc) { if (doc.type == 'rating') { emit([doc._id], doc); }}"
       },
       "by_owner": {
           "map": "function(doc) { if (doc.type == 'rating') { emit([doc.owner], doc); } }"
       },
       "by_set": {
           "map": "function(doc) { if (doc.type == 'rating') { emit([doc.setId], doc); } }"
       }
   }
}